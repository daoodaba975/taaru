        }
      },
      {
        "scope": ["markup.deleted"],
        "settings": {
          "foreground": "#E83C6A"
        }
      },
      {
        "scope": ["markup.changed"],
        "settings": {
          "foreground": "#FFB86C"
        }
      },
      {
        "scope": ["invalid"],
        "settings": {
          "foreground": "#E83C6A",
          "fontStyle": "underline italic"
        }
      },
