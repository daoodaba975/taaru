      },
      {
        "scope": ["strong"],
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["header"],
        "settings": {
          "foreground": "#A978FF"
        }
      },
      {
        "scope": ["meta.diff", "meta.diff.header"],
        "settings": {
          "foreground": "#6272A4"
        }
      },
